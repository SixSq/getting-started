 {kernel,[{inet_dist_listen_min,9100},{inet_dist_listen_max,9139}]},
 {sasl,[{sasl_error_logger,false}]},
 {lager,
     [{handlers,
          [{lager_console_backend,info},
           {lager_file_backend,
               [{file,"/var/log/op_worker/error.log"},
                {level,error},
                {size,1048576000},
                {date,"$M1D1"},
                {count,1000}]},
           {lager_file_backend,
               [{file,"/var/log/op_worker/info.log"},
                {level,info},
                {size,104857600},
                {date,"$W5D23"},
                {count,100}]},
           {lager_file_backend,
               [{file,"/var/log/op_worker/debug.log"},
                {level,debug},
                {size,10485760},
                {date,"$D0"},
                {count,10}]}]},
      {error_logger_hwm,200},
      {async_threshold,500},
      {async_threshold_window,50}]},
 {cluster_worker,
     [{datastore_config_plugin,datastore_config_plugin},
      {node_manager_plugin,node_manager_plugin},
      {dns_worker_plugin,dns_worker_plugin},
      {dns_port,53},
      {dns_a_response_ttl,60},
      {dns_ns_response_ttl,600},
      {edns_max_udp_size,1280},
      {dns_tcp_acceptor_pool_size,20},
      {dns_tcp_timeout_seconds,20}]}].