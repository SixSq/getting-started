      {default_write_event_size_threshold,10485760},
      {default_read_event_counter_threshold,100},
      {default_read_event_time_threshold_miliseconds,500},
      {default_read_event_size_threshold,10485760},
      {default_dir_mode,509},
      {default_file_mode,436},
      {ls_chunk_size,100},
      {rename_file_chunk_size,102400},
      {start_rtransfer_on_init,true},
      {direct_io_helper_threads_number,100},
      {ceph_helper_threads_number,100},
      {s3_helper_threads_number,100},
      {enable_luma_proxy,false},
      {luma_hostname,'127.0.0.1'},
