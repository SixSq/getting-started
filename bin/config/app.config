[{op_worker,
     [{oz_domain,"onedata.example.com"},
      {provider_domain,"localhost.local"},
      {verify_oz_cert,false},
      {db_nodes,['node.localhost.local:11211']},
      {cm_nodes,['cm@node.localhost.local']},
      {web_ssl_cert_path,"/etc/op_worker/certs/onedataServerWeb.pem"},
      {protocol_handler_ssl_cert_path,
          "/etc/op_worker/certs/onedataServerFuse.pem"},
      {nif_prefix,"priv"},
      {protocol_handler_pool_size,100},
      {protocol_handler_port,5555},
      {provider_protocol_handler_port,5556},
      {protocol_handler_bind_addr,loopback},
